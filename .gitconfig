[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[user]
	name = AnasIsmai1
	email = 174011704+AnasIsmai1@users.noreply.github.com
[core]
	editor = nvim
[init]
	defaultBranch = main
[core]
	pager = delta
	autocrlf = input

[interactive]
	diffFilter = delta --color-only

[add.interactive]
  useBuiltin = false # required for git 2.37.0

[delta]
	navigate = true
	side-by-side = true
  light = false

[merge]
	confilctstyle = diff3

[diff]
  colorMoved = default
